    "@bpmn-io/dmn-migrate": "^0.5.0",
    "@types/dat.gui": "^0.7.13",
    "@types/koa": "^2.15.0",
    "@types/node": "^18.19.44",
    "@types/node-wav": "^0.0.3",
    "css-loader": "^6.11.0",
    "rimraf": "^5.0.10",
    "style-loader": "^3.3.4",
    "ts-loader": "^3.2.0",
    "typescript": "^5.5.4",
    "webpack": "^5.93.0",
