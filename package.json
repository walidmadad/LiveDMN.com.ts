  "scripts": {
    "backend": "node dist/backend/Main.js",
    "build": "npx tsc --build",
    "build_backend": "npx tsc -b ts/backend",
    "build_frontend": "npx tsc -b ts/frontend",
    "clean": "rimraf dist node_modules package-lock.json",
    "migrate": "npx @bpmn-io/dmn-migrate-cli --output DMN_Specification_Chapter_11_Example.dmn ./TEST_CASES/Trisotech/DMN_Specification_Chapter_11_Example/DMN_Specification_Chapter_11_Example.dmn",
    "packaging": "webpack --config LiveDMN.config.js",
    "production": "npm run build && npm run packaging",
    "setup_TensorFlow.js_Windows": "node --eval \"const fs = require('fs'); const data = fs.readFileSync('node_modules/@tensorflow/tfjs-node/lib/napi-v9/tensorflow.dll'); fs.writeFileSync('node_modules/@tensorflow/tfjs-node/lib/napi-v8/tensorflow.dll', data);\""
  },
  "dependencies": {
    "@tensorflow/tfjs": "^4.22.0",
    "@tensorflow/tfjs-node": "^4.20.0",
    "@tensorflow/tfjs-vis": "^1.5.1",
    "dat.gui": "^0.7.9",
    "dmn-js": "^14.7.1",
    "dmn-moddle": "^10.0.0",
    "double-metaphone": "^2.0.1",
    "feelin": "^3.1.0",
    "fuse.js": "^7.0.0",
    "imicros-feel-interpreter": "^0.0.10",
    "koa": "^2.15.3",
    "node-wav": "^0.0.2",
    "papaparse": "^5.4.1",
    "partial-json-parser": "^1.2.2",
    "tar": "^7.4.0",
