    <semantic:decision id="_97ac2bfd-bda2-4507-8b95-539ccab60db0" name="Base Vacation Days">
        <semantic:description>&lt;p&gt;Every employee receives at least 22 days.&lt;/p&gt;</semantic:description>
        <semantic:variable name="Base Vacation Days" id="_afe48c8a-51ff-424f-91ad-9dbc5abfccc5" typeRef="number"/>
        <semantic:literalExpression id="_f1d202f6-7f9d-4442-9ba5-f574081e4ca8" triso:descriptionVisible="false" typeRef="number" triso:expressionId="_7b4873cf-919c-4cb9-8a6c-c8cc5056eaa1">
